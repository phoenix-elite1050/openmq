     <Match>
        <Class name="com.sun.messaging.jmq.jmsclient.XAResourceForRA"/>
        <Method name="printXid"/>
        <Bug pattern="DMI_INVOKING_TOSTRING_ON_ARRAY"/>
    </Match> 

