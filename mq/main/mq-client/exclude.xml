        <Bug pattern="NP_NULL_ON_SOME_PATH"/>
    </Match>
    
    <Match>
        <Class name="com.sun.messaging.jmq.jmsclient.ValueConvert"/>
        <Method name="toDouble"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH"/>
    </Match>
    
    <Match>
        <Class name="com.sun.messaging.jmq.jmsclient.ValueConvert"/>
        <Method name="toFloat"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH"/>
    </Match>

