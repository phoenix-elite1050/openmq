<Component xmlns="http://openinstaller.org/dependency/V1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://bindings.install.sun.com/Component ../schemas/Comp
onent.xsd">

  <!-- NSPR dependency file  -->

  <Identity>
    <Identifier feature="true">NSPR</Identifier>
    <Description>Netscape Portable Runtime</Description>
    <IdentityVersion>
      <VersionBaseline>4.8</VersionBaseline>
    </IdentityVersion>
    <Vendor>Oracle</Vendor>
    <Other>Java Enterprise System</Other>
  </Identity>

  <Payload>
    <CompositeUnit>
      <Name>core</Name>

      <!-- Solaris sparc and sparcv9 -->
      <Realization critical="true">
        <OS>
          <Type>unix</Type>
          <Name>SunOS</Name>
          <Arch>sparc</Arch>
          <OtherArch>sparcv9</OtherArch>
          <VersionCertified>
            <VersionBaseline>5.9</VersionBaseline>
          </VersionCertified>
        </OS>
        <InstallableUnit version="4.8">SUNWpr</InstallableUnit>
      </Realization>

      <!-- Solaris x86 and x86_64 -->
      <Realization critical="true">
        <OS>
          <Type>unix</Type>
          <Name>SunOS</Name>
          <Arch>i386</Arch>
          <OtherArch>x86_64</OtherArch>
          <VersionCertified>
            <VersionBaseline>5.9</VersionBaseline>
          </VersionCertified>
        </OS>
        <InstallableUnit version="4.8">SUNWpr</InstallableUnit>
      </Realization>

      <!-- Linux x86 and x86_64 -->
      <Realization critical="true">
        <OS>
          <Type>unix</Type>
          <Name>linux</Name>
          <Arch>i386</Arch>
          <OtherArch>x86_64</OtherArch>
          <VersionCertified>
            <VersionBaseline>2.4</VersionBaseline>
          </VersionCertified>
        </OS>
        <InstallableUnit version="4.8-2">sun-nspr-4.8.6-2.i386.rpm</InstallableUnit>
      </Realization>

      <Realization critical="true">
        <OS>
          <Type>unix</Type>
          <Name>linux</Name>
          <Arch>x86_64</Arch>
          <VersionCertified>
            <VersionBaseline>2.4</VersionBaseline>
          </VersionCertified>
        </OS>
        <InstallableUnit version="4.8-2">sun-nspr-4.8.2-2.i386.rpm</InstallableUnit>
        <InstallableUnit version="4.8-2">sun-nspr-4.8.6-2.x86_64.rpm</InstallableUnit>
      </Realization>

    </CompositeUnit>
  </Payload>
</Component>

